            // Get a NodeList of all list items with the "menu-item" class
          var listItems = document.querySelectorAll('.menu-item');

          // Loop through each list item in the NodeList
          listItems.forEach(function(item) {
            // Add a "click" event listener to each list item
            item.addEventListener('click', function() {
              // Remove the "menu-item-active" class from all list items
              listItems.forEach(function(li) {
                li.classList.remove('menu-item-active');
              });
              // Add the "menu-item-active" class to the clicked list item
              this.classList.add('menu-item-active');
            });
          });
</script>

